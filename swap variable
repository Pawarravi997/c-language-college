#include <stdio.h>

int main() {
    int a, b, temp;

    a = 10;
    b = 20;

    printf("Before swapping:\n");
    printf("a = %d\nb = %d\n", a, b);

    // Swapping
    temp = a;
    a = b;
    b = temp;

    printf("After swapping:\n");
    printf("a = %d\nb = %d\n", a, b);

    return 0;
}

o/p
Before swapping:
a = 10
b = 20
After swapping:
a = 20
b = 10
